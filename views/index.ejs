<% include ./partials/header %>
<div class="ui container" >
    <h3>A list of my blogs, yo.</h3>
    <% blogs.forEach((blog) => { %>
        <h2><%= blog.title%></h2>
        <img src="<%= blog.image %>" alt="">
        <p><%= blog.body.substring(0, 80) %>...</p>
        <a href="/blogs/<%= blog._id %>">Read More...</a>
    <% }); %>
</div>

    <% include ./partials/footer %>